{
    "rules": [
      { "from": "string", "to": ["string", "any"] },
      { "from": "number", "to": ["number", "any"] },
      { "from": "boolean", "to": ["boolean", "any"] },
      { "from": "json", "to": ["json", "any"] },
      { "from": "array", "to": ["array", "json", "any"] },

      { "from": "file", "to": ["file", "any"] },
      { "from": "image", "to": ["image", "file", "any"] },
      { "from": "audio", "to": ["audio", "file", "any"] },

      { "from": "matrix", "to": ["matrix", "array", "json", "any"] },
      { "from": "vector", "to": ["vector", "array", "json", "any"] },
      { "from": "ml_model", "to": ["ml_model", "any"] },
      { "from": "token", "to": ["token", "string", "any"] },
      { "from": "prompt", "to": ["prompt", "string", "any"] },
      { "from": "embedding", "to": ["embedding", "vector", "array", "any"] },

      { "from": "script", "to": ["script", "string", "any"] },
      { "from": "html", "to": ["html", "string", "any"] },
      { "from": "node_ref", "to": ["node_ref", "any"] },

      { "from": "dataset", "to": ["dataset", "any"] },
      { "from": "features", "to": ["features", "any"] },
      { "from": "labels", "to": ["labels", "any"] },
      { "from": "model", "to": ["model", "ml_model", "any"] },
      { "from": "metrics", "to": ["metrics", "json", "any"] },
      { "from": "predictions", "to": ["predictions", "array", "json", "any"] },
      { "from": "service", "to": ["service", "any"] },

      { "from": "any", "to": ["any"] },
      { "from": "null", "to": ["null"] }
    ]
  }
